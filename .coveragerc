[run]
branch = True
source = icemac.ab.document

[report]
precision = 2
omit = */generations/*
exclude_lines =
    pragma: no cover
    class I[A-Z]\w+\((zope\.interface\.Interface|I[A-Z].*)\):
    def test_.+[0-9]_webdriver\(.*
    class PO\w+\(DocumentWebdriverPageObjectBase\):

[html]
directory = htmlcov
